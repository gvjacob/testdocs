{
  "name": "testdocs",
  "displayName": "testdocs",
  "description": "",
  "publisher": "Gino",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.testdocs"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "TestDocs Configuration",
      "properties": {
        "testdocs.testFileName": {
          "type": [
            "string",
            "array"
          ],
          "default": [
            "__name__.test.js",
            "test.js"
          ],
          "markdownDescription": "",
          "description": "TestDocs scans these files from the directory of the symbol for the test cases. If one of the files is found, the rest will be ignored."
        },
        "testdocs.ignoredBlocks": {
          "type": [
            "string",
            "array"
          ],
          "default": []
        }
      }
    },
    "commands": [
      {
        "command": "extension.testdocs",
        "title": "Enable testdocs"
      }
    ]
  },
  "extensionDependencies": [
    "vscode.javascript"
  ],
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "eslint": "^5.13.0",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "monet": "^0.9.0"
  }
}
